@model ShopNow.ViewModels.ShopEditViewModel
@{
    ViewBag.Title = "Shop Edit";
    var mapkey = Session["googleMApkey"];
}
<title>ShopNow | Shop Update</title>
<style>
    #map {
        height: 100%;
        position: inherit;
    }

    .btn-file {
        position: relative;
        overflow: hidden;
    }

        .btn-file input[type=file] {
            position: absolute;
            top: 0;
            right: 0;
            min-width: 100%;
            min-height: 100%;
            font-size: 100px;
            text-align: right;
            filter: alpha(opacity=0);
            opacity: 0;
            outline: none;
            background: white;
            cursor: inherit;
            display: block;
        }

    .selBox {
        width: 200% !important;
        height: 100% !important;
    }
</style>
<div class="px-3 row col-md-12">
    <div class="col-md-6 row text-center">
        @if (Model.Status == 1 && Model.Verify == false && Model.PhoneNumber != "" && Model.PhoneVerify == false)
        {
            <div class="col-md-4 generateOTP">
                <button type="button" id="OTP" class="btn btn-block btn-success">Generated OTP</button>
            </div>
            <div class="col-md-4 generateOTP">
                <input type="text" class="form-control" id="OTPNumber" name="OTPNumber" value="@Model.Otp" readonly />
            </div>
        }
        @if (Model.Status == 1 && Model.Verify == false)
        {
            <div class="col-md-12 d-none documentVerifyPending">
                <div class="alert alert-danger font-weight-bold p-2">Document Verification Pending!</div>
            </div>
            <div class="col-md-6 mt-2 d-none verifyOTP">
                <h6 class="text-success"> OTP Verified</h6>
            </div>
        }
        @if (Model.Status == 1 && Model.Verify == true && Model.Password != null)
        {
            <div class="col-md-12 NotverifyOTP">
                <div class="alert alert-warning font-weight-bold p-2">Please Go to <a href="https://shopnowpay.com/" target="_blank">ShopNowPay</a> and Enter your Products for Shop Activation.</div>
            </div>
        }
        @if (Model.Status == 1 && Model.Verify == false && (Model.PhoneNumber != "" || Model.PhoneNumber != null) && Model.PhoneVerify == false)
        {
            <div class="col-md-4 PhoneVerify">
                <h6 class="text-success">Phone Verification</h6>
                <span>CALL from <span class="text-danger">@Model.PhoneNumber</span> TO <span class="text-warning">04448134440</span></span><br /><span>and verify your Phone number</span>
            </div>
        }
    </div>
    @*@if (Model.Status == 1 && Model.Verify == true )
        {*@
    <div class="col-md-6">
        <div class="row text-center">
            <div class="col-md-4">
                <button type="button" id="GeneratePassword" class="btn btn-success showPasswordtext">Generate Password</button>
                <button type="button" class="btn btn-success disablePasswordtext" disabled>Password</button>
            </div>
            <div class="col-md-4">
                <input type="text" class="form-control" id="Password" value="@Model.Password" readonly />
            </div>
            <div class="col-md-4">
                <label class="font-weight-bold text-secondary"> Platform Credit : </label> <span class="fa fa-rupee text-secondary"></span> <span class="text-danger">@Model.PlatformCredit</span>
                <label class="font-weight-bold text-secondary"> Delivery Credit : </label> <span class="fa fa-rupee text-secondary"></span> <span class="text-danger">@Model.DeliveryCredit</span>
            </div>
        </div>
    </div>
    @*}*@
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <h5 class="text-success" style="letter-spacing:1px">SHOP UPDATE</h5>
        </div>
        <div class="col-md-6 text-right">
            <a href="~/Shop/List" target="_blank"><span class="fa fa-bars"></span>&nbsp;Shop List</a>
        </div>
    </div>
    <div class="row mb-3">
        <div class="col-md-12 mx-auto">
            <div class="card shadow">
                <div class="card-body">
                    @using (Html.BeginForm("Edit", "Shop", FormMethod.Post, new { id = "ShopForm", role = "form", enctype = "multipart/form-data" }))
                    {
                        @Html.AntiForgeryToken()
                        <input type="hidden" name="Id" id="Id" value="@Model.Id" />
                        <input type="hidden" name="CustomerId" id="CustomerId" value="@Model.CustomerId" />
                        <input type="hidden" name="ImageLogoPath" id="ImageLogoPath" value="@Model.ImageLogoPath" />
                        <input type="hidden" name="ImagePath" id="ImagePath" value="@Model.ImagePath" />
                        <input type="hidden" name="ImageAadharPath" id="ImageAadharPath" value="@Model.ImageAadharPath" />
                        <input type="hidden" name="ImageAccountPath" id="ImageAccountPath" value="@Model.ImageAccountPath" />
                        <input type="hidden" name="ImageGSTINPath" id="ImageGSTINPath" value="@Model.ImageGSTINPath" />
                        <input type="hidden" name="ImagePanPath" id="ImagePanPath" value="@Model.ImagePanPath" />
                        <input type="hidden" name="ImageFSSAIPath" id="ImageFSSAIPath" value="@Model.ImageFSSAIPath" />
                        <input type="hidden" name="ImageDrugPath" id="ImageDrugPath" value="@Model.ImageDrugPath" />
                        <input type="hidden" name="ImageEstablishPath" id="ImageEstablishPath" value="@Model.ImageEstablishPath" />
                        <input type="hidden" name="ImageOtherLicensePath" id="ImageOtherLicensePath" value="@Model.ImageOtherLicensePath" />
                        <input type="hidden" name="ImageAuthoriseBrandPath" id="ImageAuthoriseBrandPath" value="@Model.ImageAuthoriseBrandPath" />
                        <input type="hidden" name="Count" id="Count" value="@Model.Count" />
                        <input type="hidden" name="Status" id="Status" value="@Model.Status" />
                        <div class="card border-0 mb-4">
                            <div class="card-body row">
                                <div class="col-md-6">
                                    <div class="row ml-2">
                                        <input class="col-11 mb-2" id="pac-input" type="text" placeholder="Select your Shop" />
                                        <div id="location-error"></div>
                                        <div class="mb-4" id="map" style="width: 92%; height: 250px;"></div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <div class="col-md-4 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Name</strong></label>
                                        </div>
                                        <div class="col-md-8">
                                            <input id="Name" type="text" class="form-control" name="Name" value="@Model.Name" />
                                        </div>
                                    </div>
                                    <div class="form-group row mobile">
                                        <div class="col-md-4 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Mobile Number</strong></label>
                                        </div>
                                        <div class="col-md-8">
                                            <input id="PhoneNumber" type="text" class="form-control" name="PhoneNumber" value="@Model.PhoneNumber" readonly />
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-4 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Official Email</strong></label>
                                        </div>
                                        <div class="col-md-8">
                                            <input id="OfficialEmail" type="text" class="form-control" name="OfficialEmail" value="@Model.OfficialEmail" />
                                        </div>
                                    </div>
                                    <div class="form-group row website">
                                        <div class="col-md-4 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Website</strong></label>
                                        </div>
                                        <div class="col-md-8">
                                            <input id="Website" type="text" class="form-control" name="Website" value="@Model.Website" readonly />
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-4 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Shop Type</strong></label>
                                        </div>
                                        <div class="col-md-8">
                                            <select id="ShopCategoryId" name="ShopCategoryId" class="form-control" style="width:100%;">
                                                <option value="@Model.ShopCategoryId">@Model.ShopCategoryName</option>
                                            </select>
                                            <input type="hidden" name="ShopCategoryName" id="ShopCategoryName" value="@Model.ShopCategoryName" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <div class="col-md-2 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Address</strong></label>
                                        </div>
                                        <div class="col-md-10">
                                            <textarea class="form-control" name="Address" id="Address" style="resize: none;" readonly>@Model.Address</textarea>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-2 mt-2 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Location</strong></label>
                                        </div>
                                        <div class="col-md-5">
                                            <small class="control-label col-sm-12" for="Latitude"><i class="text-secondary">Latitude</i></small>
                                            <input id="Latitude" type="text" class="form-control mb-2" placeholder="Latitude" name="Latitude" value="@Model.Latitude" readonly />
                                        </div>
                                        <div class="col-md-5">
                                            <small class="control-label col-sm-12" for="Longitude"><i class="text-secondary">Longitude</i></small>
                                            <input id="Longitude" type="text" class="form-control" placeholder="Longitude" name="Longitude" value="@Model.Longitude" readonly />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <div class="col-md-4">
                                            <small class="control-label col-sm-12" for="StreetName"><i class="text-secondary">Locality</i></small>
                                            <input id="StreetName" type="text" class="form-control" placeholder="Street Name" name="StreetName" value="@Model.StreetName" />
                                        </div>
                                        <div class="col-md-4">
                                            <small class="control-label col-sm-12" for="DistrictName"><i class="text-secondary">District</i></small>
                                            <input id="DistrictName" type="text" class="form-control" placeholder="District Name" name="DistrictName" value="@Model.DistrictName" />
                                        </div>
                                        <div class="col-md-4">
                                            <small class="control-label col-sm-12" for="StateName"><i class="text-secondary">State / Province</i></small>
                                            <input id="StateName" type="text" class="form-control" placeholder="State Name" name="StateName" value="@Model.StateName" />
                                        </div>
                                        <div class="col-md-4">
                                            <small class="control-label col-sm-12" for="CountryName"><i class="text-secondary">Country</i></small>
                                            <input id="CountryName" type="text" class="form-control" placeholder="Country Name" name="CountryName" value="@Model.CountryName" />
                                        </div>
                                        <div class="col-md-4">
                                            <small class="control-label col-sm-12" for="PinCode"><i class="text-secondary">PIN Code</i></small>
                                            <input id="PinCode" type="text" class="form-control" placeholder="PIN Code" name="PinCode" value="@Model.PinCode" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <div class="col-md-4 text-center">
                                            <label class="text-secondary font-weight-bold ml-2 text-center">Shop Image</label><br />
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="img-upload-container ml-4">
                                                        <img class="img-container w-100 h-100" id="img-upload" src="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImagePath" onerror="this.src='/Images/noimage.png'" />
                                                        <input type="file" id="ShopImage" name="ShopImage" class="d-none" />
                                                        <label for="ShopImage" class="img-browse"><span class="fa fa-plus"></span> Select Image</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row text-center">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-sm btn-danger" id="btnShopRemove">Remove</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 text-center">
                                            <label class="text-secondary font-weight-bold ml-2 text-center">Logo Image</label><br />
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="img-upload-container ml-4">
                                                        <img class="img-container w-100 h-100" id="img-logo" src="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageLogoPath" onerror="this.src='/Images/noimage.png'" />
                                                        <input type="file" id="LogoImage" name="LogoImage" class="d-none" />
                                                        <label for="LogoImage" class="img-browse"><span class="fa fa-plus"></span> Select Image</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row text-center">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-sm btn-danger" id="btnLogoRemove">Remove</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-12 form-group row">
                                        <div class="col-md-5 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Google Rating</strong></label>
                                        </div>
                                        <div class="col-md-7">
                                            <input id="Rating" type="text" class="form-control" placeholder="Rating" name="Rating" value="@Model.Rating" readonly />
                                        </div>
                                    </div>
                                    <div class="col-md-12 form-group row">
                                        <div class="col-md-5 text-md-left">
                                            <label class="col-form-label"><strong class="text-secondary">Google Review</strong></label>
                                        </div>
                                        <div class="col-md-7">
                                            <input id="CustomerReview" type="text" class="form-control" placeholder="Customer Review" name="CustomerReview" value="@Model.CustomerReview" readonly />
                                        </div>
                                    </div>
                                    <div class="col-md-12 form-group row">
                                        <div class="col-md-5 text-md-left">
                                            <label class="col-form-label text-secondary font-weight-bold">Adscore</label>
                                        </div>
                                        <div class="col-md-7">
                                            <input id="Adscore" type="number" min="1" value="@Model.Adscore" class="form-control" name="Adscore" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 form-group row">
                                        <div class="col-md-5 text-md-left">
                                            <label class="col-form-label text-secondary font-weight-bold">ShopPricePercentage</label>
                                        </div>
                                        <div class="col-md-7">
                                            <input id="ShopPricePercentage" type="number" min="0" value="@Model.ShopPricePercentage" class="form-control" name="ShopPricePercentage" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 form-group row">
                                        <div class="col-md-5">
                                            <label class="form-check-label text-secondary font-weight-bold">
                                                Shop Rate
                                            </label>
                                        </div>
                                        <div class="col-md-6 ml-4">
                                            <input type="checkbox" class="form-check-input" name="IsShopRate" value="true" @if (Model.IsShopRate) { <text> checked</text> } />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-4">
                            <div class="row">
                                <h5 class="text-success ml-4 mt-2">Proprietor Details</h5>
                                <div class="col-md-12 mx-auto">
                                    <div class="card-body row">
                                        <div class="col-md-7">
                                            <div class="form-group row">
                                                <div class="col-md-4 mt-4 text-md-left">
                                                    <label class="col-form-label"><strong class="text-secondary">Name</strong></label>
                                                </div>
                                                <div class="col-md-8">
                                                    <small class="control-label col-sm-12" for="AadharName"><i class="text-secondary">*  As in Aadhar</i></small>
                                                    <input id="AadharName" type="text" class="form-control" name="AadharName" value="@Model.AadharName" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-4 text-md-left">
                                                    <label class="col-form-label"><strong class="text-secondary">Aadhar Number</strong></label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input id="AadharNumber" type="text" class="form-control" name="AadharNumber" value="@Model.AadharNumber" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-4 text-md-left">
                                                    <label class="col-form-label"><strong class="text-secondary">Mobile Number *</strong></label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input id="OwnerPhoneNumber" type="text" class="form-control" name="OwnerPhoneNumber" value="@Model.OwnerPhoneNumber" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-4 text-md-left">
                                                    <label class="col-form-label"><strong class="text-secondary">Personal Email</strong></label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input id="Email" type="text" class="form-control" name="Email" value="@Model.Email" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2"></div>
                                        <div class="col-md-3">
                                            <div class="card border-primary bg-muted">
                                                <div class="card-body text-center">
                                                    <label class="text-secondary font-weight-bold ml-2 text-center">Aadhar Image</label><br />
                                                    <input type="radio" class="ml-3" name="AadharDocument" id="AadharDocumentImage" value="Image" checked> <label for="AadharDocumentImage"> Image</label>
                                                    <input type="radio" class="ml-3" name="AadharDocument" id="AadharDocumentPDF" value="PDF"> <label for="AadharDocumentPDF"> PDF</label>
                                                    <div class="row ml-3 AadharDocumentPdf d-none mt-2 mb-3">
                                                        @if (Model.ImageAadharPath != null)
                                                        {
                                                            <a href="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageAadharPath" target="_blank" class="AadharPdflink mb-2"> Click here for Pdf view</a>
                                                        }
                                                        <input type="file" id="AadharPdf" name="AadharPdf" />
                                                    </div>
                                                    <div class="row AadharDocumentImg ml-5 mt-1">
                                                        <div class="img-upload-container ml-3">
                                                            <img class="img-container w-100 h-100 mb-2" id="img-aadhar" src="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageAadharPath" onerror="this.src='/Images/noimage.png'" />
                                                            <input type="file" id="AadharImage" name="AadharImage" class="d-none" />
                                                            <label for="AadharImage" class="img-browse"><span class="fa fa-plus"></span> Select Image</label>
                                                        </div>
                                                    </div>
                                                    <div class="row mb-2">
                                                        <div class="col-md-12 ml-2 mt-1">
                                                            <button type="button" class="btn btn-sm btn-danger" id="btnAadharRemove">Remove</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-4">
                            <h5 class="text-success ml-2 mt-2">License Details</h5>
                            <div class="card-body row">
                                <div class="card-deck">
                                    <div class="card border-primary bg-muted">
                                        <div class="card-body">
                                            <label class="col-md-12 text-center"><strong class="text-secondary">FSSAI License</strong></label>
                                            <label class="col-md-12 text-left"><small class="text-secondary text-left"><i>License Number</i></small></label>
                                            <input id="FSSAINumber" type="text" class="form-control col-md-12" name="FSSAINumber" value="@Model.FSSAINumber" />
                                            <label class="col-md-12 text-left"><small class="text-secondary"><i>License Image</i></small></label>
                                            <input type="radio" class="ml-3" name="FssaiLicense" id="FssaiLicenseImage" value="Image" checked> <label for="FssaiLicenseImage"> Image</label>
                                            <input type="radio" class="ml-3" name="FssaiLicense" id="FssaiLicensePDF" value="PDF"> <label for="FssaiLicensePDF"> PDF</label>
                                            <div class="row ml-3 FssaiPdf d-none mt-2 mb-3">
                                                @if (Model.ImageFSSAIPath != null)
                                                {
                                                    <a href="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageFSSAIPath" target="_blank" class="FssaiPdflink mb-2"> Click here for Pdf view</a>
                                                }
                                                <input type="file" id="FSSAIPdf" name="FSSAIPdf" />
                                            </div>
                                            <div class="row FssaiImg mt-2">
                                                <div class="img-upload-container mx-auto mb-3">
                                                    <img class="img-container w-100 h-100 mb-2" id="img-fssai" src="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageFSSAIPath" onerror="this.src='/Images/noimage.png'" />
                                                    <input type="file" id="FSSAIImage" name="FSSAIImage" class="d-none" />
                                                    <label for="FSSAIImage" class="img-browse"><span class="fa fa-plus"></span> Select Image</label>
                                                </div>
                                            </div>
                                            <div class="row mb-2">
                                                <div class="col-md-12 text-center">
                                                    <button type="button" class="btn btn-sm btn-danger" id="btnFssaiRemove">Remove</button>
                                                    @if (Model.ImageFSSAIPath != null)
                                                    {
                                                        <button type="button" class="btn btn-sm btn-warning" id="btnFssaiExpand" data-toggle="modal" data-target="#FssaiModal">
                                                            + Expand
                                                        </button>
                                                        <input type="hidden" id="FSSIApprove" name="FSSIApprove" />
                                                    }
                                                </div>
                                            </div>
                                            @if (Model.ImageFSSAIPath != null && Model.Verify == false)
                                            {
                                                <div class="row divFssaiLabels">
                                                    <div class="col-md-12">
                                                        <div class="alert alert-success text-center FssaiApprovedlbl font-weight-bold d-none p-2">Approved</div>
                                                        <div class="alert alert-danger text-center FssaiRejectedlbl font-weight-bold d-none p-2">Rejected</div>
                                                        @if (Model.ImageFSSAIPath == "Rejected" || Model.FSSAINumber == "Rejected")
                                                        {
                                                            <div class="alert alert-danger text-center Fssai font-weight-bold p-2">Rejected</div>
                                                        }
                                                        else
                                                        {
                                                            <div class="alert alert-danger text-center Fssai font-weight-bold p-2">Verification Pending</div>
                                                        }
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="card border-primary bg-muted">
                                        <div class="card-body">
                                            <label class="col-md-12 text-center"><strong class="text-secondary">Drug License</strong></label>
                                            <label class="col-md-12 text-left"><small class="text-secondary"><i>License Number</i></small></label>
                                            <input id="DrugNumber" type="text" class="form-control" name="DrugNumber" value="@Model.DrugNumber" />
                                            <label class="col-md-12 text-left"><small class="text-secondary"><i>License Image</i></small></label>
                                            <input type="radio" class="ml-3" name="DrugLicense" id="DrugLicenseImage" value="Image" checked> <label for="DrugLicenseImage"> Image</label>
                                            <input type="radio" class="ml-3" name="DrugLicense" id="DrugLicensePDF" value="PDF"> <label for="DrugLicensePDF"> PDF</label>
                                            <div class="row ml-3 DrugPdf d-none mt-2 mb-3">
                                                @if (Model.ImageDrugPath != null)
                                                {
                                                    <a href="https://s3.ap-south-1.amazonaws.com/shopnowchat.com/Medium/@Model.ImageDrugPath" target="_blank" class="DrugPdflink mb-2"> Click here for Pdf view</a>
                                                }
                                                <input type="file" id="DrugPdf" name="DrugPdf" />
                                            </div>
