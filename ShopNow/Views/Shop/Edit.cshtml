@section Scripts
{
    <!--Select2-->
    <script src="~/Scripts/select2/js/select2.js"></script>
    <link href="~/Scripts/select2/css/select2.css" rel="stylesheet" />
    <link href="~/Content/select2/select2.custom.css" rel="stylesheet" />
    <!--Select2 End-->
    <script src="~/Scripts/multiple-image-upload.js"></script>
    <link href="~/Content/multiple-image-upload.css" rel="stylesheet" />
    <script src="~/Scripts/sweetalert/sweetalert.min.js"></script>
    <link href="~/Content/sweetalert/sweetalert.css" rel="stylesheet" />
    <script src="https://maps.googleapis.com/maps/api/js?key=@mapkey&libraries=places&callback=initMap" async defer></script>
    <script>

        function printDiv(divName) {
            var printContents = document.getElementById(divName).innerHTML;
            w = window.open();
            w.document.write(printContents);
            w.print();
            w.close();
        }

        $(function () {
            $('#OTP').click(function () {
                var mobileNo = $('#OwnerPhoneNumber').val();
                var Id = $("#Id").val();
                $.getJSON("/Shop/GenerateOTP?MobileNo=" + mobileNo + "&Id=" + Id, function (data) {
                    $('#OTPNumber').val(data.data);
                    if (data.data != null) {
                        $("#OTP").attr('disabled', true);
                    }
                    if (data.ErrorMessage != null) {
                        alert(data.ErrorMessage);
                    }
                });

            });
        });

        $(function () {
            $('#GeneratePassword').click(function () {
                var Id = $('#CustomerId').val();
                $.getJSON("/Shop/GeneratePassword?Id=" + Id, function (data) {
                    $('#Password').val(data.data);
                    $("#GeneratePassword").attr('disabled', true);
                    $(".showPasswordtext").addClass("d-none");
                    $(".disablePasswordtext").removeClass("d-none");
                    location.reload();
                });
            });
        });

        $(document).ready(function () {
            var type = $("#DeliveryType").val();
            var parceldroptype = $("#ParcelDropDeliveryType").val();
            var tiertype = $("#DeliveryTierType").val();
